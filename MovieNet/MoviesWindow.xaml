<Page x:Class="MovieNet.MoviesWindow"
      xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
      xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
      xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
      xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
      xmlns:local="clr-namespace:MovieNet"
      mc:Ignorable="d" 
      d:DesignHeight="450" d:DesignWidth="800"
      Title="MoviesWindow">

    <Grid>
        <Rectangle Fill="#FFF4F4F5" Margin="0,69,10,10" Stroke="Black" HorizontalAlignment="Right" Width="780"/>
        <ListBox ItemsSource="{Binding Movies}" HorizontalAlignment="Left" Height="100" Margin="234,269,0,0" VerticalAlignment="Top" Width="100"/>
    </Grid>
</Page>
